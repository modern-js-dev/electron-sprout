{
  "name": "@modern-js/electron-test",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/electron-sprout/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/modern-js-dev/electron-sprout.git",
    "directory": "packages/electron-test"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.0-rc.20",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./main": "./dist/js/node/index.main.js",
    "./render": "./dist/js/node/index.render.js",
    "./webview": "./dist/js/node/index.webview.js"
  },
  "typesVersions": {
    "*": {
      "main": [
        "./dist/types/index.main.d.ts"
      ],
      "render": [
        "./dist/types/index.render.d.ts"
      ],
      "webview": [
        "./dist/types/index.webview.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "new": "modern new",
    "build": "modern build"
  },
  "peerDependencies": {
    "@modern-js/electron-runtime": "workspace:>=1.0.0-rc.22",
    "@modern-js/electron-tools": "workspace:>=1.0.0-rc.20"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "jest-environment-node": "^27.0.6"
  },
  "devDependencies": {
    "@modern-js/module-tools": "^1.0.0-rc.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.11",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@modern-js/electron-runtime": "workspace:^1.0.0-rc.22",
    "@modern-js/electron-tools": "workspace:^1.0.0-rc.20",
    "typescript": "~4.3.5"
  },
  "jupiterSettings": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
