{
  "extends": ["@modern-js-app"],
  // eslint-plugin-import 的规则现在有 bug，可以临时加上：
  "rules": {
    "import/export": "off",
    "no-var": "off",
    "no-console": "off",
    "accessor-pairs": "off",
    // need annotation
    "filenames/match-exported": "off",
    // such as data && data[key]
    "@typescript-eslint/prefer-optional-chain": "off",
    // want to use promise
    "promise/prefer-await-to-then": "off",
    // need two class in a file
    "max-classes-per-file": "off",
    // packages/electron-runtime/src/core/instantiation/instantiationService.ts
    "no-useless-call": "off",
    // packages/electron-runtime/src/core/instantiation/instantiationService.ts
    "@typescript-eslint/restrict-template-expressions": "off",
    // packages/electron-runtime/src/core/base/node/processes.ts
    "@typescript-eslint/no-unnecessary-type-assertion": "off",
    // packages/electron-runtime/src/core/instantiation/instantiationService.ts
    "new-cap": "off",
    "no-proto": "off",
    // IOC
    "@typescript-eslint/no-parameter-properties": "off",
    // service define need
    "@typescript-eslint/no-redeclare": "off",
    // handleVetos need
    "@typescript-eslint/no-loop-func": "off",
    // packages/electron-runtime/src/services/lifecycle/electron-main/lifecycle.ts
    "@typescript-eslint/typedef": "off",
    // packages/electron-runtime/src/services/lifecycle/electron-main/lifecycle.ts
    "@typescript-eslint/member-ordering": "off",
    // packages/electron-runtime/src/services/lifecycle/electron-main/lifecycle.ts
    "@typescript-eslint/no-misused-promises": "off",
    // packages/electron-runtime/src/services/lifecycle/electron-main/lifecycle.ts
    "@typescript-eslint/adjacent-overload-signatures": "off",
    // packages/electron-runtime/src/services/lifecycle/electron-main/lifecycle.ts
    "promise/param-names": "off",
    // packages/electron-runtime/src/core/base/parts/ipc/common/ipc.net.ts
    "@typescript-eslint/prefer-literal-enum-member": "off",
    // /packages/electron-runtime/src/core/instantiation/instantiation.ts#46
    "@typescript-eslint/ban-types": "off",
    // packages/electron-runtime/src/core/base/common/strings.ts#1174
    "@typescript-eslint/naming-convention": "off",
    // packages/electron-runtime/src/core/base/common/event.ts
    "@typescript-eslint/method-signature-style": "off",
    // packages/electron-runtime/src/core/base/common/functional.ts
    "@babel/no-invalid-this": "off",
    "radix": "off",
    "@typescript-eslint/no-invalid-void-type": "off",
    "consistent-return": "off",
    "import/extensions": ["off"],
    "import/named": "off",
    "import/no-commonjs": "off",
    "@typescript-eslint/interface-name-prefix": "off",
    "jsdoc/require-example": "off",
    "no-new": "off",
    "promise/no-nesting": "off",
    "@typescript-eslint/ban-ts-ignore": "off",
    "no-shadow": "off",
    "no-useless-constructor": "off",
    "max-statements": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/require-await": "off",
    "no-template-curly-in-string": "off",
    "no-bitwise": "off",
    "max-lines": "off",
    "callback-return": "off",
    "no-dupe-class-members": "off",
    "import/no-mutable-exports": "off",
    "no-param-reassign": "off",
    "@typescript-eslint/no-namespace": "off",
    "no-inner-declarations": "off",
    "@typescript-eslint/camelcase": "off",
    "@typescript-eslint/unified-signatures": "off",
    "no-useless-escape": "off",
    "@typescript-eslint/no-empty-function": "off",
    "jsdoc/check-alignment": "off",
    "max-depth": "off",
    "@typescript-eslint/prefer-string-starts-ends-with": "off",
    "consistent-this": "off",
    "babel/no-invalid-this": "off",
    "@typescript-eslint/no-this-alias": "off",
    "prefer-rest-params": "off",
    "jsdoc/require-description": "off",
    "default-case": "off",
    "no-lonely-if": "off",
    "no-control-regex": "off",
    "jsdoc/check-param-names": "off",
    "complexity": "off",
    "@typescript-eslint/prefer-for-of": "off",
    "no-multi-assign": "off",
    "prefer-destructuring": "off",
    "no-nested-ternary": "off",
    "@typescript-eslint/prefer-regexp-exec": "off",
    "max-params": "off",
    "valid-typeof": "off",
    "jsdoc/check-tag-names": "off",
    "@typescript-eslint/class-name-casing": "off",
    "@typescript-eslint/no-require-imports": "off",
    "no-useless-concat": "off",
    "handle-callback-err": "off",
    "@typescript-eslint/no-var-requires": "off",
    "no-case-declarations": "off",
    "max-len": "off",
    "jsdoc/require-param-description": "off",
    "no-loop-func": "off",
    "babel/no-unused-expressions": "off",
    "@typescript-eslint/no-use-before-define": "off",
    "operator-assignment": "off",
    "@typescript-eslint/prefer-readonly": "off",
    "@typescript-eslint/consistent-type-assertions": "off",
    "@typescript-eslint/no-extraneous-class": "off",
    "@typescript-eslint/no-empty-interface": "off",
    "@typescript-eslint/no-shadow": "off"
  }
}
